{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["ES2016"],
    "moduleResolution": "Node10",
    "module": "CommonJS",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  }
}
