{
  "name": "@mfs/shell",
  "type": "module",
  "version": "1.0.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./contract": {
      "require": "./dist/contract.cjs",
      "import": "./dist/contract.js"
    }
  },
  "typesVersions": {
    "*": {
      "contract": [
        "./dist/contract.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "rxjs": "^7.8.1",
    "@mfs/utility": "*"
  }
}
